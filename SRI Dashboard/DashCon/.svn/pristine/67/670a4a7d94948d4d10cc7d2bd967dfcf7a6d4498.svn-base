angular.module('sri.controller.SaferLookup', ['sri.module.Effects'])
	.controller('SaferLookupCTRL', ['$scope', '$http', '$rootScope', '$timeout', function( $scope, $http, $rootScope, $timeout ) {
		$scope.truck = $scope.truck || {};
		var _this = this;
