angular.module('sri.controller.StaticScale', [])
	.controller('StaticScaleCTRL', ['$rootScope', '$scope', '$http', '$timeout', function( $rootScope, $scope, $http, $timeout ) {
		$scope.truck = $scope.truck || {};
		$scope.scale = $scope.scale || {};
