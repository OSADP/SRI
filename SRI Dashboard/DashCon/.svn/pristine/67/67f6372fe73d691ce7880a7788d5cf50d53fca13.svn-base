/* index.jsp */

* {
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

html, body {
	height: 100% !important;
	padding: 0;
	margin: 0; overflow: auto;
}

.navbar-inverse {
  border-radius: 0 !important;
  -moz-border-radius: 0 !important;
}

.navbar-custom {
	position: relative;
	margin: -50px 0 0 0 !important;
	height: 62px;
	padding-top: 5px;
	width: 100%;
	background: #22313F;
	//position: absolute;
	//background: rgba( 255, 255, 255, .1) !important;
	//border-bottom: solid 1px rgba( 255, 255, 255, .2);
	border-radius: 0;
	transition: .5s;
	z-index: 10;
}			
.navbar-custom .navbar-brand {
	color: rgba(236, 240, 241, 1);
	text-shadow: 0 1px 0 rgba( 0, 0, 0, 1);
	font-size: 1.8em;
}
.navbar-custom .navbar-brand:hover {
	color: rgba(236, 240, 241, 1);
}
.nav > li > a:focus {
	background: none;
}
.navbar-custom li a, .navbar-custom li a:focus {
	color: rgb(236, 240, 241) !important;
	border: 3px solid rgba( 52, 73, 94, 0 );
	border-radius: 0;
	text-shadow: 0 0px 2px rgba( 0, 0, 0, 1);
	font-size: 1.1em;
	font-family: 'Istok Web', sans-serif;
	font-weight: 700;
}
.navbar-custom li a:hover {
	background: rgba( 255, 255, 255, 0 ) !important;
	color: rgb(236, 240, 241) !important;
}
.navbar-custom li a.home:hover {
	border-bottom: 3px solid rgb(241, 196, 15);
}
.navbar-custom li a.about:hover {
	border-bottom: 3px solid rgb(52, 152, 219);
}
.navbar-custom li a.dashcon:hover {
	border-bottom: 3px solid rgb(192, 57, 43);
}
.navbar-custom li a.login:hover {
	border-bottom: 3px solid rgb(39, 174, 96);
}
.navbar-toggle .icon-bar {
	background-color: rgba(236, 240, 241, 1) !important;
}
.navbar-fixed-top .icon-bar {
	background-color: rgba(44, 62, 80, 1) !important;
}
.navbar-custom.navbar-fixed-top {
	position: fixed;
	margin: 0 !important;
	background: #ecf0f1;
}
.navbar-fixed-top li a, .navbar-fixed-top .navbar-brand {
	color: #2c3e50 !important;
	text-shadow: none;
}
.navbar-fixed-top li a:hover, .navbar-fixed-top .navbar-brand:hover {
	color: #2c3e50 !important;
}
