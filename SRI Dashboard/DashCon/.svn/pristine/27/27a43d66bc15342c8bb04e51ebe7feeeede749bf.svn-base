		<script type="text/template" id="create-user-template">
			<form action="" id="formRegister" class="col-sm-12 col-md-6 col-lg-4 clearfix" autocomplete="off">
				<div class="form-group">
					<label for="user_id">User ID:</label>
					<input type="text" id="userId" class="form-control" name="user_id" required placeholder="User Id is required" />
					<span id="err_userId" class="help-block"></span>
				</div>
				<div class="form-group">
					<label for="password">Password:</label>
					<input type="password" id="password" class="form-control" name="password" placeholder="Password is required" />
					<span id="err_password" class="help-block"></span>
				</div>
				<div class="form-group">
					<label for="first_name">First Name:</label>
					<input type="text" id="firstName" class="form-control" name="first_name" placeholder="Firstname is required" />
					<span id="err_firstName" class="help-block"></span>
				</div>
				<div class="form-group">
					<label for="last_name">Last Name:</label>
					<input type="text" id="lastName" class="form-control" name="last_name" placeholder="Lastname is required" />
					<span id="err_lastName" class="help-block"></span>
				</div>
				<div class="form-group">
					<label for="email">Email:</label>
					<input type="email" id="email" class="form-control" name="email" placeholder="Email is required" />
					<span id="err_email" class="help-block"></span>
				</div>
				<div class="form-group">
					<label for="state_id">State ID:</label>
					<select id="stateId" class="form-control" name="state_id"></select>
					<span class="help-block"></span>
				</div>
				<label for="user_role">Role:</label>
				<div class="checkbox">
					<label class="checkbox-inline">
						<input type="radio" name="user_role" value="admin"> Admin
					</label>
					<br />
					<label class="checkbox-inline">
						<input type="radio" name="user_role" value="user"> User
					</label>
					<span class="help-block"></span>
				</div>
				<button class="btn btn-sm btn-primary" id="create" type="submit" ng-click="createUser()">
					<span class="glyphicon glyphicon-ok"></span> 
					Create
				</button>
				<a ui-sref="listUsers()" class="btn btn-sm btn-default" id="cancel">
					<span class="glyphicon glyphicon-remove"></span>
					Back
				</a>
			</form>
		</script>