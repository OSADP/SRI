

App.View.Overview = Backbone.View.extend({
	
	template : template('module-overview'),
	
	initialize : function() {
		App.Event.Vehicle.on( "load", this.refresh, this );
	},
	
	events : {
		
	},
	
	refresh : function ( model ) {
		this.model = model;
		this.render();
		return this;
	},
	
	render: function () {
		this.$el.empty();
		this.$el.html( this.template( this.model.attributes ) );
		return this;
	}
	
});