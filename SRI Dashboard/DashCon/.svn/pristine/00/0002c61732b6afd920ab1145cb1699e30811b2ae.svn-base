<script type="text/template" id="list-site-template">
	<td>{{ id }}</td>
	<td>{{ name }}</td>
	<td>{{ state }}</td>
	<td>{{ siteType }}</td>
	<td class="hidden-xs hidden-sm">{{ description }}</td>
	<td class="hidden-xs hidden-sm"> Lat: {{ latitude }}, Lon: {{ longitude }} </td> 
	<td>
		<a href="#/edit/{{ id }}" class="btn btn-xs btn-warning"><span class="glyphicon glyphicon-pencil" title="Edit"></span></a>
		<button class="btn btn-xs btn-danger" id="delete"><span class="glyphicon glyphicon-trash" title="Delete"></span></button>
	</td>
</script>