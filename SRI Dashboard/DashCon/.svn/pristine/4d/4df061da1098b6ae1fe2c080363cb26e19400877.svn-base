.navbar-collapse {
  background-color: rgba(34, 49, 63, 0.8);
  box-shadow: none;
}
.full-height {
  height: auto;
  min-height: 100%;
}
#mainContent {
  color: #ecf0f1;
  height: auto;
  margin-top: 10%;
}
#mainContent .main-text {
  font-family: Visby Demi;
}
.stage {
  padding: 0;
  padding-top: 50px;
  background: url(/DashCon/images/dark-shader.png) repeat;
  background-color: rgba(129, 207, 224, 0.2);
}
.loader-mask {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #22313f;
  top: 0;
  left: 0;
  z-index: 9998;
}
.loader-logo {
  position: absolute;
  width: 100px;
  height: 100px;
  top: 50%;
  left: 50%;
  margin-top: -50px;
  margin-left: -50px;
}
.transform {
  backface-visibility: visible;
  transform-origin: 50% 50% 0px;
  animation: revolve 2s linear 0s infinite;
  -ms-animation: revolve 2s linear 0s infinite;
  -moz-animation: revolve 2s linear 0s infinite;
  -webkit-animation: revolve 2s linear 0s infinite;
}
@-webkit-keyframes revolve {
  0% {
    transform: perspective(100px) rotateY(0deg);
  }
  50% {
    transform: perspective(100px) rotateY(-180deg);
  }
  100% {
    transform: perspective(100px) rotateY(-360deg);
  }
}
@-moz-keyframes revolve {
  0% {
    transform: perspective(100px) rotateY(0deg);
  }
  50% {
    transform: perspective(100px) rotateY(-180deg);
  }
  100% {
    transform: perspective(100px) rotateY(-360deg);
  }
}
@-o-keyframes revolve {
  0% {
    transform: perspective(100px) rotateY(0deg);
  }
  50% {
    transform: perspective(100px) rotateY(-180deg);
  }
  100% {
    transform: perspective(100px) rotateY(-360deg);
  }
}
@keyframes revolve {
  0% {
    transform: perspective(100px) rotateY(0deg);
  }
  50% {
    transform: perspective(100px) rotateY(-180deg);
  }
  100% {
    transform: perspective(100px) rotateY(-360deg);
  }
}
