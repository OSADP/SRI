/* index.jsp */
@font-face {
  font-family: 'Manifold';
  src: url('../fonts/manifold/ManifoldCF-Regular-webfont.eot?') format('eot'), url('../fonts/manifold/ManifoldCF-Regular-webfont.woff') format('woff'), url('../fonts/manifold/ManifoldCF-Regular-webfont.ttf') format('truetype');
}
@font-face {
  font-family: 'Visby Demi';
  src: url('../fonts/visby-demi/VisbyCF-DemiBold.eot?') format('eot'), url('../fonts/visby-demi/VisbyCF-DemiBold.woff') format('woff'), url('../fonts/visby-demi/VisbyCF-DemiBold.ttf') format('truetype');
}
.material {
  display: block;
  position: absolute;
  background: #ffffff;
  border-radius: 100%;
  transform: scale(0);
}
.material.animate {
  animation: ripple 0.65s linear;
}
@keyframes ripple {
  100% {
    opacity: 0;
    transform: scale(2.5);
  }
}
* {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
html,
body {
  height: 100% !important;
  padding: 0;
  margin: 0;
  overflow: auto;
}
.navbar-inverse {
  border-radius: 0 !important;
  -moz-border-radius: 0 !important;
}
.navbar-custom {
  position: relative;
  margin: -30px 0 0 0 !important;
  height: 62px;
  padding-top: 5px;
  width: 100%;
  background: #22313F;
  border-radius: 0;
  transition: .5s;
  z-index: 10;
  background: transparent;
}
.navbar-custom .navbar-collapse {
  background: transparent;
}
@media screen and (max-width: 768px) {
  .navbar-custom .navbar-collapse {
    background: rgba(0, 0, 0, 0.8);
  }
}
.navbar-custom .navbar-brand {
  color: #ecf0f1;
  text-shadow: 0 1px 0 #000000;
  font-size: 1.8em;
}
.navbar-custom .navbar-brand:hover,
.navbar-custom .navbar-brand:focus {
  color: #ecf0f1;
}
.navbar-custom .navbar-brand:hover .brand-logo,
.navbar-custom .navbar-brand:focus .brand-logo {
  animation: sri-brand 0.3s ease-in;
}
.navbar-custom .navbar-brand .brand-logo {
  max-height: 40px;
  max-width: 42px;
  opacity: 1;
}
@keyframes sri-brand {
  70% {
    max-width: 47px !important;
  }
  100% {
    max-width: 42px !important;
  }
}
.navbar-custom .navbar-nav a,
.navbar-custom .navbar-nav a:focus {
  background: transparent !important;
  color: #ecf0f1 !important;
  border: 3px solid rgba(52, 73, 94, 0);
  border-radius: 0;
  text-shadow: 0 0px 2px #000000;
  font-size: 1.1em;
  font-family: 'Istok Web', sans-serif;
  font-family: Visby Demi;
  letter-spacing: 1pt;
  font-weight: 700;
  transition: .5s;
}
.navbar-custom .navbar-nav a span {
  display: block;
  text-align: center;
}
.navbar-custom .navbar-nav a.home:hover,
.navbar-custom .navbar-nav a.home:focus {
  border-bottom: 3px solid #f1c40f;
}
.navbar-custom .navbar-nav a.about:hover,
.navbar-custom .navbar-nav a.about:focus {
  border-bottom: 3px solid #3498db;
}
.navbar-custom .navbar-nav a.dashcon:hover,
.navbar-custom .navbar-nav a.dashcon:focus {
  border-bottom: 3px solid #c0392b;
}
.navbar-custom .navbar-nav a.login:hover,
.navbar-custom .navbar-nav a.login:focus {
  border-bottom: 3px solid #27ae60;
}
.navbar-toggle .icon-bar {
  background-color: #ecf0f1 !important;
}
.navbar-fixed-top .icon-bar {
  background-color: #2c3e50 !important;
}
.navbar-custom.navbar-fixed-top {
  position: fixed;
  margin: 0 !important;
  background: #ecf0f1;
}
.navbar-fixed-top li a,
.navbar-fixed-top .navbar-brand {
  color: #2c3e50 !important;
  text-shadow: none;
}
.navbar-fixed-top li a:hover,
.navbar-fixed-top .navbar-brand:hover {
  color: #2c3e50 !important;
}
#mainLogo,
#mainLogo2 {
  margin-top: -10px;
}
.container-header {
  padding: 0;
  padding-top: 30px;
  margin: 0 !important;
  color: #ecf0f1;
}
.container-header2 {
  position: relative;
  padding: 0;
  height: 550px;
  background: url(/DashCon/images/sri-main-header-background-2.jpg) center center no-repeat;
  background-size: cover;
  color: #ecf0f1;
}
.dark-theme {
  background: #2c3e50;
  color: #ecf0f1;
}
.light-theme {
  background: #ecf0f1;
  color: #2c3e50;
}
.container.jumbotron {
  background: none;
  width: 100%;
  padding: 0;
  margin: 0;
  font-family: 'Raleway', sans-serif;
  text-align: center;
}
.slider-nav {
  font-size: 3em;
  color: rgba(255, 255, 255, 0.5);
}
.slider-nav:hover {
  color: #ffffff;
}
.btn-transparent {
  color: #FFF;
  border: solid 1px #FFF;
  transition: .3s;
  padding: 11px 17px;
}
.btn-transparent:hover {
  color: #FFF;
  background: rgba(255, 255, 255, 0.2);
}
.btn-opaque {
  background: rgba(255, 255, 255, 0.8);
  color: #2c3e50;
  border: solid 2px rgba(255, 255, 255, 0);
  margin-right: 15px;
  transition: .3s;
}
.btn-opaque:hover {
  background: #ffffff;
}
video {
  display: block;
}
