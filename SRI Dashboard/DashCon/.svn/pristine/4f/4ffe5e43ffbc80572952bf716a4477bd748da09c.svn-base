

#saferLookupModule {

	.module-box {
		background: #6C7A89;
	  	border: none;
  	}
  	
  	h4 {
  		color: #f8f8f8 !important;
		border-bottom: 4px solid #6C7A89;
		padding-bottom: 16px;
	}
	
	.btn-clipboard {
		color: #f8f8f8;
		margin-bottom: 4px !important;
	}
	
	.well {
		padding: 0 !important;
	}
	
	.form-group {
		margin: 0;
	}
	
	input {
	  border: none;
	  border-left: 1px solid #b1c2c6;
	  border-right: 1px solid #b1c2c6;
	  transition: .5s;
	  height: 50px;
	  border-radius: 0;
	  font-size: 16px;
	  text-align: center;
	  
	  &:focus {
	  	box-shadow: none;
	  }
	  
	}
	
	.btn-lg {
		border-radius: 0;
		font-size: 16px;
		width: 50%;
	}
	
	.btn-success {
		float: left;
	}
}