

<script type="text/template" id="module-static-scale">
						
	<div class="col-xs-12 module-wrapper" id="staticScaleModule">
		
		<div class="module-box {{ status }}">
	
			<div style="position: relative">
				
				<button id="btnSettings" class="btn btn-sm btn-inverse btn-clipboard " title="Settings">
					<span class="glyphicon glyphicon-cog"></span>
				</button>

				<button id="btnRefresh" class="btn btn-sm btn-inverse btn-clipboard " title="Refresh">
					<span class="glyphicon glyphicon-refresh"></span>
				</button>

			</div>
			
			<h4>Static Scale:</h4>
			
			<div class="well">
				
				<p>Axle Count: <strong><span class="text-info pull-right">{{ axleCount }}</span></strong></p>
				<p>Declared Weight: <strong><span class="text-info pull-right">{{ declaredWeight }}</span></strong></p>
				<p>Gross Weight: <strong><span class="text-info pull-right">{{ grossWeight }}</span></strong></p>
				<p>Status: <strong><span class="text-info text-status pull-right">{{ status }}</span></strong></p>
				<p>Reason: <strong><span class="text-info pull-right">{{ reason }}</span></strong></p>
			
			</div>

		</div>
		
	</div>
	
</script>