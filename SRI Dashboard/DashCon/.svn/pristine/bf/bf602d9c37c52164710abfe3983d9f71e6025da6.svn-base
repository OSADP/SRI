/****************************

	File: top-navigation.less
	
	Desc: These are rules that are general which means
	it can be used in many cases.
	
	Author: Robert Roth

****************************/
.navbar.navbar-inverse {
  background-color: #2f4048;
  border-bottom-color: #11171a;
}
.navbar.navbar-inverse .navbar-brand {
  color: #d9dcde;
}
.navbar.navbar-inverse .dropdown,
.navbar.navbar-inverse .dropdown-toggle {
  background: #2f4048;
}
.navbar.navbar-inverse li > a {
  color: #bdc3c7;
}
.navbar.navbar-inverse ul.dropdown-menu {
  background: #2f4048;
  border-color: #11171a;
}
.navbar.navbar-inverse ul.dropdown-menu li > a:hover {
  background: #253239;
}
/****************************

	File: sidebar.less
	
	Desc: These are the styles for the Sidebar and
	elements within	the SideBar.	
	
	Author: Robert Roth

****************************/
#sideBar {
  background: #2f4048;
  color: #ecf0f1;
  padding-left: 0px;
  padding-right: 0px;
}
#sideBar .badge {
  background-color: #11171a;
}
#sideBar .nav > li > a:hover,
#sideBar .nav > li > a:focus {
  background: none;
}
#sideBar ul.dropdown-menu {
  background: #435b67;
  border-color: #253239;
}
#sideBar ul.dropdown-menu li > a:hover {
  background: #2f4048;
}
#sideBar .nav-pills {
  background-color: #253239;
  border-bottom: 4px solid #11171a;
}
#sideBar .nav-pills li a {
  font-size: 12px;
  color: #bdc3c7;
}
#sideBar .nav-pills li a.text-success {
  color: #27ae60;
}
#sideBar .nav-pills li a.text-warning {
  color: #e67e22;
}
#sideBar .nav-pills li a.text-fail {
  color: #c0392b;
}
#sideBar .truck-entry-controls {
  position: fixed;
  width: 447px;
  border-bottom: 1px solid #11181f;
  z-index: 999;
}
#sideBar .truck-entry-controls .btn-group {
  margin: 0;
  padding: 0;
}
#sideBar .truck-entry-controls .btn.dropdown-toggle {
  margin: 0;
  border-radius: 0;
}
#sideBar .truck-entry-controls .dropdown-menu {
  margin-top: 0;
}
#sideBar .truck-entry-controls .dropdown-menu a {
  font-size: 12px;
}
#sideBar .truck-entry-controls .btn-group > .dropdown-menu li a.text-success {
  color: #27ae60;
}
#sideBar .truck-entry-controls .btn-group > .dropdown-menu li a.text-warning {
  color: #e67e22;
}
#sideBar .truck-entry-controls .btn-group > .dropdown-menu li a.text-fail {
  color: #c0392b;
}
#sideBar .entry-wrapper {
  position: relative;
  height: auto;
  width: 100%;
  margin: 0;
  padding-top: 31px;
  overflow: auto;
}
#sideBar .truck-entry-box {
  position: relative;
  height: 100px;
  width: 100%;
  background: #2f4048;
  color: #bdc3c7;
  border-bottom: 1px solid #11171a;
  cursor: pointer;
  transition: .3s;
}
#sideBar .truck-entry-box:hover {
  background-color: #394e57;
}
#sideBar .truck-entry-box.passed {
  background: #1e8449;
  color: #ecf0f1;
}
#sideBar .truck-entry-box.passed:hover {
  background: #229955;
}
#sideBar .truck-entry-box.warned {
  background: #bf6516;
  color: #ecf0f1;
}
#sideBar .truck-entry-box.warned:hover {
  background: #d67118;
}
#sideBar .truck-entry-box.failed {
  background: #962d22;
  color: #ecf0f1;
}
#sideBar .truck-entry-box.failed:hover {
  background: #ab3326;
}
#sideBar .truck-entry-box .truck-info {
  min-width: 200px;
  padding-left: 10px;
  padding-top: 10px;
  height: 100px;
  font-family: 'Open Sans', sans-serif;
}
#sideBar .truck-entry-box .truck-thumbnail {
  position: relative;
  height: 99px;
  width: 120px;
  background: url(../truck-thumbnails/001.png);
  background-size: cover;
}
/****************************
	
	File: main-content.less
	
	Desc: This contains styles for the #mainContent
	interface.
	
	Author: Robert Roth

****************************/
#mainContent {
  background: #cfd9db;
  color: #555555;
}
#mainContent .column {
  padding: 0 !important;
}
#mainContent #top,
#mainContent #middle,
#mainContent #bottom {
  padding-bottom: 10px !important;
}
#mainContent #mainMenu > li > a:hover,
#mainContent #mainMenu > li > a:focus {
  background: #c0cdd1;
}
#mainContent .module-wrapper {
  padding: 10px;
  padding-right: 0;
  padding-bottom: 0;
}
#mainContent #left .module-wrapper {
  padding-left: 0 !important;
}
#mainContent .module-menu {
  position: relative;
}
#mainContent .module-box {
  background: #ecf0f1;
  padding: 10px;
  border: 1px solid #b1c2c6;
  border-radius: 3px;
  transition: .5s;
  box-shadow: inset 0 0 2px rgba(255, 255, 255, 0.7), inset 0 1px 0 rgba(255, 255, 255, 0.5);
}
#mainContent .module-box .well {
  border-color: #cacaca;
  margin-bottom: 0;
  padding-bottom: 10px;
}
#mainContent .module-box .well p {
  color: #636363;
  border-bottom: solid 1px #CCC;
  margin-bottom: 7px;
}
#mainContent .module-box .well .text-status {
  text-transform: uppercase;
}
#mainContent .module-box .well .text-status.failed {
  color: #c0392b;
}
#mainContent .module-box .well .text-status.passed {
  color: #27ae60;
}
#mainContent .module-box.failed {
  background: #c0392b !important;
