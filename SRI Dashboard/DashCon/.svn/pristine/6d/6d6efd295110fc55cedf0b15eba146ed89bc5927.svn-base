
	
<div class="col-xs-12 module-wrapper" id="saferUSCrashModule" ng-controller="USCrashDataCTRL">
	
	<div class="module-box">

		<div class="box-header clearfix">

			<div id="btnRefresh" class="btn-clipboard unbutton pull-left" title="Refresh">
				<span class="glyphicon glyphicon-refresh" ng-class></span>
			</div>
			
			<button id="btnSettings" class="btn btn-sm btn-clipboard pull-right" title="Settings">
				<span class="fa fa-bars"></span>
			</button>
		
			<h4>SAFER US Crash Data</h4>

		</div>
			
		<div class="loading-bar" ng-class="{ 'loading': crashData.loading }"></div>
		
		<div class="well">
			<table id="crashDataTable" class="table table-bordered">
				<thead>
					<tr>
						<th>Fatal</th>
						<th>Injury</th>
						<th>Tow</th>
						<th>Total</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>{{ crashData.fatal || '-' }}</td>
						<td>{{ crashData.injury || '-' }}</td>
						<td>{{ crashData.tow || '-' }}</td>
						<td>{{ crashData.total || '-' }}</td>
					</tr>
				</tbody>
			</table>
		</div>
		
	</div>
	
</div>