

<script type="text/template" id="module-carrier-info">
						
	<div class="col-xs-12 module-wrapper" id="carrierInfo">
		
		<div class="module-box module-blue">
	
			<div style="position: relative">
				
				<button id="btnSettings" class="btn btn-sm btn-inverse btn-clipboard " title="Settings">
					<span class="glyphicon glyphicon-cog"></span>
				</button>

				<button id="btnRefresh" class="btn btn-sm btn-inverse btn-clipboard " title="Refresh">
					<span class="glyphicon glyphicon-refresh"></span>
				</button>

			</div>
			
			<h4>Carrier Information:</h4>
			
			<div class="well">
				
				<p>I.D.: <span class="text-info pull-right"><strong>{{ carrierId }}</strong></span></p>
				<p>Name: <span class="text-info pull-right"><strong>{{ carrierName }}</strong></span></p>
			
			</div>
			
		</div>
		
	</div>
	
</script>