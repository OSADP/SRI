  background-image: -moz-linear-gradient(90deg, rgba(142, 68, 173, 0.1), rgba(112, 54, 136, 0.1));
  background-image: -ms-linear-gradient(90deg, rgba(142, 68, 173, 0.1), rgba(112, 54, 136, 0.1));
  background-image: -o-linear-gradient(90deg, rgba(142, 68, 173, 0.1), rgba(112, 54, 136, 0.1));
  background-image: linear-gradient(90deg, rgba(142, 68, 173, 0.1), rgba(112, 54, 136, 0.1));
}
.list-group-item.active,
.list-group-item.active:hover,
.list-group-item.active:focus {
