  background-image: -webkit-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: -moz-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: -ms-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: -o-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  border-color: #7f3d9b !important;
}
.list-group-item.selected {
  background-color: rgba(142, 68, 173, 0.05) !important;
  background-image: -webkit-linear-gradient(90deg, rgba(142, 68, 173, 0.05), rgba(112, 54, 136, 0.05));
  background-image: -moz-linear-gradient(90deg, rgba(142, 68, 173, 0.05), rgba(112, 54, 136, 0.05));
  background-image: -ms-linear-gradient(90deg, rgba(142, 68, 173, 0.05), rgba(112, 54, 136, 0.05));
  background-image: -o-linear-gradient(90deg, rgba(142, 68, 173, 0.05), rgba(112, 54, 136, 0.05));
  background-image: linear-gradient(90deg, rgba(142, 68, 173, 0.05), rgba(112, 54, 136, 0.05));
}
.btn-primary {
  background-color: #8e44ad !important;
  background-image: -webkit-linear-gradient(90deg, #8e44ad, #703688);
  background-image: -moz-linear-gradient(90deg, #8e44ad, #703688);
  background-image: -ms-linear-gradient(90deg, #8e44ad, #703688);
  background-image: -o-linear-gradient(90deg, #8e44ad, #703688);
  background-image: linear-gradient(90deg, #8e44ad, #703688);
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  -moz-background-clip: padding;
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  border: none !important;
}
.btn-primary:hover,
.btn-primary:focus {
  background-color: #a563c1 !important;
  background-image: -webkit-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: -moz-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: -ms-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: -o-linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
  background-image: linear-gradient(90deg, #a563c1, rgba(41, 128, 185, 0.3));
}
.form-control:focus {
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(142, 68, 173, 0.75);
  -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(142, 68, 173, 0.75);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(142, 68, 173, 0.75);
  border-color: rgba(41, 128, 185, 0.3);
}
