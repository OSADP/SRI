
	
<div class="col-xs-12 module-wrapper" id="saferUSInspectionsModule" ng-controller="USInspectionsCTRL">
	
	<div class="module-box">

		<div class="box-header clearfix">

			<div id="btnRefresh" class="btn-clipboard unbutton pull-left" title="Refresh">
				<span class="glyphicon glyphicon-refresh" ng-class></span>
			</div>
			
			<button id="btnSettings" class="btn btn-sm btn-clipboard pull-right" title="Settings">
				<span class="fa fa-bars"></span>
			</button>
		
			<h4>SAFER US Inspections</h4>
			
		</div>
			
		<div class="loading-bar" ng-class="{ 'loading': usInspectionData.loading }"></div>
		
		<div class="well">
			<p>Driver: <span class="text-info pull-right"><strong>{{ usInspectionData.driver  || '-' }}</strong></span></p>
			<p>Hazardous Materials: <span class="text-info pull-right"><strong>{{ usInspectionData.hazmat  || '-' }}</strong></span></p>
			<p>Intermodal Equipment Provider: <span class="text-info pull-right"><strong>{{ usInspectionData.iep  || '-' }}</strong></span></p>
			<p>Vehicle: <span class="text-info pull-right"><strong>{{ usInspectionData.vehicle  || '-' }}</strong></span></p>
		</div>
		
	</div>
	
</div>