

'use strict';


angular.module('sri.controller.Safer.SafetyRating', [])
	.controller('SafetyRatingCTRL', ['$scope', function( $scope ) {
		$scope.safetyRating = $scope.safetyRating || {};
		
		$scope.$on('saferDataLoaded', function( event, data ) {
			if( typeof data == 'object' && data.safetyRating != undefined ) {
				$scope.safetyRating = data.safetyRating;
			} else {
				$scope.safetyRating = '';
			}
		});
	}]);