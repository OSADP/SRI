            <div id="ajaxLoader" ng-hide="loaderShow">
              <img src="/DashCon/images/bar-loader.gif" id="" class="pull-right" alt="loader" />
            </div>
            <!-- /// -->
          </section>
          
        </div>
        <!-- // UserProfileCtrl -->
        
        <!-- THIS WILL BE THE CONTAINER FOR OUR VIEWS -->
<!--        <div id="mainContainer" ui-view>             -->
<!--          THIS OUR LOADER IMAGE -->
<!--          <div id="ajaxLoader" ng-hide="loaderShow"> -->
<!--            <img src="../images/bar-loader.gif" id="" class="pull-right" alt="loader" /> -->
<!--          </div> -->
<!--          /// -->
<!--        </div> -->
        <!-- /// -->
      </div>
      
    </div>
    
    <!-- FOOTER -->
    <div ng-include="'./includes/FooterTmpl.html'" ng-controller="profile.NavigationCtrl as nav"></div>
    <!-- // -->
    
    <!-- JS SCRIPTS -->
    <!-- CONTROLLERS -->
    <script src="./scripts/controllers/NavigationCtrl.js"></script>
    <script src="./scripts/controllers/EditCtrl.js"></script>
    <script src="./scripts/controllers/MainCtrl.js"></script>
    <!-- // -->
