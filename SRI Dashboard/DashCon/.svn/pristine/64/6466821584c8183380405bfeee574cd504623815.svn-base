<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=Edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
		
		<title>SRI - Dashboard</title>
		<!-- FAVICON -->
		<link rel="icon" type="image/png" href="/DashCon/images/favicon.png" />
		<!-- OPEN SANS FONT ( 300, 400 | Italic, 600 ) | NOTE: Google Font -->
		<link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,400italic,600' rel='stylesheet' type='text/css'>
		<!-- CUSTOM BOOTSTRAP FRAMEWORK | NOTE: www.getboostrap.com for Docs. -roth -->
		<link rel="stylesheet" href="/DashCon/vendor/bootstrap/custom/css/bootstrap.css" />
		<!-- NOTE: main.css contains all styles compiled using less, 1 http call = faster rendering. -->
		<link rel="stylesheet" href="styles/main.css" />
		
		<!-- Animation effects -->
		<link rel="stylesheet" href="../vendor/animate/animate.css" />
		<!-- FontAwesome -->
		<link rel="stylesheet" href="../vendor/font-awesome/css/font-awesome.css" />
		
		<!-- OTHER STYLES | NOTE: I don't advice adding other external file, learn LESS, pretty easy. -->
			<!-- insert css here -->
		<!-- END OTHER STYLES-->
		
	</head>
	<body ng-app="SRI.DASHBOARD">
	<!-- // -->
	<!-- BEGIN MAIN WORK AREA -->
		<div id="wrapper" class="container-fluid sticky-footer-wrap">
		
			<div id="main" class="row sticky-footer-main" ng-controller="MainCTRL as main">
				
				<!-- TOP NAVIGATION -->
				<jsp:include page="includes/top-navigation.jsp"></jsp:include>
				<!-- // -->
				
				<div id="sideBar" ng-controller="TruckFeedCTRL as truckfeed" class="col-lg-3 col-md-5 col-sm-12 col-xs-12 wow fadeInDown">
					
					<jsp:include page="includes/truck-entry-controls.jsp"></jsp:include>
					
					<div id="vehicleEntries" class="entry-wrapper">
						<div ng-include="'templates/TruckFeedTMPL.html'"></div>
					</div>
					
				</div>
				
				<!-- MAIN CONTENT | NOTE: This is where the modules are at. -roth-->
				<div id="mainContent" class="col-lg-9 col-md-7 hidden-sm hidden-xs">
					
				
					<div id="frameContainer"class="col-xs-12 col-sm-4 hidden">
						<button id="frameCloser" class="btn btn-sm btn-danger">
							<span class="glyphicon glyphicon-remove-sign"></span>
						</button>
						<iframe src="http://micjin.state.mi.us"></iframe>
					</div>
				
					<div id="mainLoader" class="hidden" style="position: fixed; height: 100%; width: 100%; background: rgba( 0, 0, 0, .3); left: 0; z-index: 999;">
						<div style="position: absolute;background: #FFF; height: 100px; width: 100px; top: 50%; left: 50%; margin: -120px 0 0 -50px; z-index: 1000; border-radius: 50%;">
							<img src="/DashCon/images/circular-loader.gif" alt="Loading" style="position: relative; top: 50%; left: 50%; margin: -32px 0 0 -32px;" />
						</div>
					</div>
					
					<div class="row" style="padding: 10px 10px 0 20px;" ng-controller="RightMenuCTRL">
						<div ng-include="'templates/RightTopMenuTMPL.html'"></div>
					</div>
					
					<!-- NOTE: For now we have 3 logical columns and
						 rows are now dynamic. www.getbootstrap.com for grid
						 layout documentation. -roth -->
					<div id="left_panel" class="column col-xs-12 col-md-12 col-lg-4 wow fadeInUp" ng-controller="LeftPanelCTRL">
					</div>
					<div id="middle_panel" class="column col-xs-12 col-md-12 col-lg-4 wow fadeInUp">
					</div>
					<div id="right_panel" class="column col-xs-12 col-md-12 col-lg-4 wow fadeInUp">
					</div>
				
				</div>
				<!-- BACK TO TOP BUTTON | NOTE: we might not need it. -roth -->
				<span class="hidden glyphicon glyphicon-circle-arrow-up back-to-top-button" title="Back to top"></span>
				
			</div>
		</div>
		<!-- END MAIN WORK AREA -->
		<!-- // -->
		
		<!-- STICKY FOOTER | NOTE: I made changes from the original sticky footer code, 
			 so it ain't so sticky anymore. -roth -->
		<div id="footer" class="sticky-footer hidden-xs">
			<p class="pull-right"><a href="http://www.leidos.com">leidosâ„¢</a></p>
			<p><a href="/DashCon">Smart Roadside Initiative</a></p>
		</div>
		<!-- // -->
		
		<div id="mobileTrigger" class="visible-xs visible-sm">
			<button id="phoneSideBar" class="btn btn-block btn-primary">
				<span class="fa fa-toggle-off"></span>
				Show Modules
			</button>
			<button id="phoneModules" class="btn btn-block btn-primary" style="display:none; margin-top: 0px">
				<span class="fa fa-toggle-on"></span>
				Show Truck Feed
			</button>
		</div>
		
		<!-- OTHER DEPENDECIES -->
		<script src="/DashCon/vendor/jquery/jquery-1.11.0.js"></script>
		<script type="text/javascript" src="/DashCon/vendor/wowjs/wow.js"></script>
		<script src="/DashCon/vendor/moment/moment.js"></script>
		<script src="/DashCon/vendor/bootstrap/custom/js/bootstrap.js"></script>
		<script type="text/javascript" src="/DashCon/vendor/wowjs/wow.js"></script>
		
		<!-- LOAD ANGULAR EARLY, NOT ADVISED BUT THIS AVOIDS EXPOSURE OF EXPRESSIONS -->
		<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.27/angular.js" type="text/javascript"></script>
		<script type="text/javascript">
		//<![CDATA[
		(window.angular)||document.write('<script type="text/javascript" src="/DashCon/vendor/angular/angular.js"><\/script>');//]]>
		</script>
		<!-- // -->
		
		<!-- START APPLICATION -->
		<script type="text/javascript" src="scripts/app.js"></script>
		<!-- CONTROLLERS -->
		<script src="scripts/controllers/sri.dashboard.controller.Main.js"></script>
		<script src="scripts/controllers/sri.dashboard.controller.TruckFeed.js"></script>
		<script src="scripts/controllers/sri.dashboard.controller.SidebarControls.js"></script>
		<script src="scripts/controllers/sri.dashboard.controller.RightTopMenu.js"></script>
		<script src="scripts/controllers/sri.dashboard.controller.LeftPanel.js"></script>
		<script src="scripts/controllers/sri.dashboard.controller.Module.js"></script>
		<!-- MODULES -->
		<script src="modules/sri.dashboard.controller.Modules.js"></script>
		<script src="modules/wim/sri.dashboard.controller.RampWIM.js"></script>
		<script src="modules/truck-phone/sri.dashboard.controller.TruckPhoneInfo.js"></script>
		<script src="modules/safer/company-data/sri.dashboard.module.CompanyData.js"></script>
		<script src="modules/safer/us-crash-data/sri.dashboard.module.CrashData.js"></script>
		<script src="modules/safer/safety-rating/sri.dashboard.module.SafetyRating.js"></script>
		<script src="modules/safer/us-inspections/sri.dashboard.module.USInspections.js"></script>
		<script src="modules/safer-lookup/sri.dashboard.controller.SaferLookup.js"></script>
		<script src="modules/static-scale/sri.dashboard.controller.StaticScale.js"></script>
		<!-- DIRECTIVES -->
		<script src="scripts/directives/sri.dashboard.directive.Module.js"></script>
		<script src="scripts/directives/modules/sri.dashboard.directive.MainlineWIM.js"></script>
		<script src="scripts/directives/modules/sri.dashboard.directive.DataCaptured.js"></script>
		<script src="scripts/mobile-helpers.js"></script>
	</body>
</html>