/**
 * @author rothrob
 * @define App, Backbone
 * @desc   Site Model
 */	
define(['app', 'backbone'], function(App, Backbone){

	App.Collections.Sites = Backbone.Collection.extend({
		model: App.Models.Site,
		
		url: '/DashCon/resources/site',
		
		parse : function(response) {
			this.reset();
			return response.site;
		}
		
	});
	
	return App;

});