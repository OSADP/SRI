
<script type="text/template" id="module-data-captured">

	<div class="col-xs-12 module-wrapper" id="dataCaptured">
		
		<div class="module-box">
			
			<div style="position: relative">
				
				<button id="btnSettings" class="btn btn-sm btn-inverse btn-clipboard " title="Settings">
					<span class="glyphicon glyphicon-cog"></span>
				</button>

				<button id="btnRefresh" class="btn btn-sm btn-inverse btn-clipboard " title="Refresh">
					<span class="glyphicon glyphicon-refresh"></span>
				</button>

			</div>
			
			<h4 class="text-primary">Data Captured:</h4>
			
			<div class="well">
				<p>License Plate: <strong><span class="text-info pull-right">{{ licensePlate }}</span></strong></p>
				<p>DOT #: <strong><span class="text-info pull-right">{{ dotNumber }}</span></strong></p>
				<p>Weight: <strong><span class="text-info pull-right">{{ weight }}</span></strong></p>
			</div>
			
		</div>
		
	</div>

</script>