

<script type="text/template" id="vehicle-entry-list">
	
	<div class="truck-entry-box {{ pass }}" title="Click to view details">
	
		<div class="truck-thumbnail pull-left"></div>
	
		<div class="truck-info pull-left">
			<p><strong>Captured:</strong> <i>11:06:45 AM 06-25-2014</i></p>
			<p><strong>Carrier:</strong> {{ carrierName }}</p>
			<p><strong>State:</strong> {{ state }}</p>
		</div>

		<span class="glyphicon glyphicon-chevron-right hidden"
			style="position: absolute; top: 50%; right:0; margin-top:-23px; margin-right: 20px; font-size: 3em;"
		></span>
		
	</div>

</script>